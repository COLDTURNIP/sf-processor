{
   "pipeline":[
     {
      "processor": "SysFlowProc",
      "handler": "Flattener",
      "in":   "sysflow SysFlowChan",
      "out":  "flat FlattenerChan"
     },
     {
      "processor": "PolicyEngine",
      "in":   "flat FlattenerChan",
      "out":  "evt EventChan",
      "policies": "/usr/local/sf-processor/conf/"  
     },
     {
      "processor": "Syslogger",
      "in":   "evt EventChan"
     }
   ]
}
