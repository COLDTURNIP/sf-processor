{
   "_comment": "Default configuration (check ../README.md for details)",
   "pipeline":[
     {
      "processor": "sysflowproc",
      "handler": "flattener",
      "in": "sysflow sysflowchan",
      "out": "flat flattenerchan"
     },
     {
      "processor": "policyengine",
      "in": "flat flattenerchan",
      "out": "evt eventchan",
      "policies": "/usr/local/sf-processor/policies/",
      "mode": "alert"
     },
     {
      "processor": "exporter",
      "in": "evt eventchan",
      "export": "terminal",
      "type": "alert",
      "format": "json",
      "flat": "false",
      "path": "./export.out",
      "proto": "tcp",
      "tag": "sysflow",
      "host": "localhost",
      "port": "514",
      "buffer": "0"
     }
   ]
}
